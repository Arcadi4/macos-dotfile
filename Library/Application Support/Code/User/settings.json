{
    "workbench.editorAssociations": {
        "*.copilotmd": "vscode.markdown.preview.editor",
        "*.pdf": "pdf.preview",
        "*.md": "default",
        "{git,gitlens}:/**/*.{md,csv}": "default",
        "{git,gitlens,git-graph}:/**/*.{md,csv,svg}": "default"
    },
    "git.autofetch": true,
    "editor.tabCompletion": "on",
    "terminal.integrated.enableMultiLinePasteWarning": "never",
    "hediet.vscode-drawio.resizeImages": null,
    "window.dialogStyle": "custom",
    "workbench.editor.empty.hint": "hidden",
    "launch": {
        "configurations": []
    },
    "editor.fontFamily": "Monaspace Neon",
    "editor.fontLigatures": "'ss01', 'ss02', 'ss03', 'ss06', 'ss07', 'calt', 'liga', 'dlig'",
    "editor.unicodeHighlight.ambiguousCharacters": true,
    "editor.fontVariations": true,
    "editor.codeLensFontFamily": "Monaspace Krypton",
    "editor.experimentalWhitespaceRendering": "font",
    "workbench.fontAliasing": "auto",
    "editor.fontWeight": "normal",
    "chat.editor.fontFamily": "Monaspace Neon",
    "debug.console.fontFamily": "Monaspace Argon",
    "scm.inputFontSize": 12,
    "terminal.integrated.fontFamily": "Monaspace Neon",
    "editor.codeActionsOnSave": {},
    "notebook.output.fontFamily": "Monaspace Radon",
    "explorer.confirmDelete": false,
    "grammarly.files.include": [
        "**/readme.md",
        "**/README.md",
        "**/*.txt",
        "**/*.md"
    ],
    "terminal.integrated.env.osx": {
        "FIG_NEW_SESSION": "1"
    },
    "python.defaultInterpreterPath": "/usr/bin/python3",
    "leetcode.workspaceFolder": "/Users/skylar/.leetcode",
    "leetcode.defaultLanguage": "cpp",
    "leetcode.hint.commentDescription": false,
    "editor.renderWhitespace": "boundary",
    "editor.bracketPairColorization.independentColorPoolPerBracketType": true,
    "security.workspace.trust.banner": "always",
    "security.workspace.trust.untrustedFiles": "open",
    "diffEditor.codeLens": true,
    "editor.minimap.autohide": true,
    "markdown.extension.tableFormatter.delimiterRowNoPadding": true,
    "javascript.format.semicolons": "insert",
    "typescript.format.semicolons": "insert",
    "markdown.extension.toc.levels": "2..6",
    "explorer.confirmDragAndDrop": false,
    "markdown.extension.theming.decoration.renderLink": true,
    "[cpp]": {
        "editor.defaultFormatter": "llvm-vs-code-extensions.vscode-clangd"
    },
    "editor.indentSize": "tabSize",
    "terminal.integrated.smoothScrolling": true,
    "terminal.integrated.lineHeight": 1.15,
    "terminal.integrated.fontWeightBold": "700",
    "terminal.integrated.fontWeight": "300",
    "markdown-pdf.printBackground": false,
    "liveServer.settings.donotVerifyTags": true,
    "markdown.extension.print.includeVscodeStylesheets": false,
    "css.format.spaceAroundSelectorSeparator": true,
    "css.format.preserveNewLines": false,
    "html.format.indentHandlebars": true,
    "html.format.indentInnerHtml": true,
    "html.format.wrapLineLength": 0,
    "html.format.preserveNewLines": false,
    "git.openRepositoryInParentFolders": "never",
    "vscode_custom_css.imports": [
        "file:///Users/skylar/.vscode.css",
        "file:///Users/skylar/.vscode/extensions/brandonkirbyson.vscode-animations-2.0.3/dist/updateHandler.js"
    ],
    "workbench.startupEditor": "none",
    "editor.formatOnSave": true,
    "markdownlint.config": {
        "MD014": false,
        "MD033": false,
        "MD041": false
    },
    "[javascript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "prettier.tabWidth": 4,
    "[jsonc]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[css]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "window.titleBarStyle": "custom",
    "[markdown]": {
        "editor.wordWrap": "bounded",
        "editor.defaultFormatter": "yzhang.markdown-all-in-one"
    },
    "[typescript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "typescript.updateImportsOnFileMove.enabled": "always",
    "animations.CursorAnimationOptions": {
        "Color": "#F7CE46",
        "TrailLength": 4
    },
    "editor.cursorSmoothCaretAnimation": "explicit",
    "workbench.list.smoothScrolling": true,
    "animations.Scrolling": "Fade",
    "[json]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "editor.smoothScrolling": true,
    "eslint.run": "onSave",
    "npm-intellisense.showBuildInLibs": true,
    "console-ninja.featureSet": "Community",
    "[vue]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "clangd.path": "/usr/bin/clangd",
    "editor.wordWrap": "bounded",
    "npm-intellisense.importQuotes": "\"",
    "path-intellisense.autoSlashAfterDirectory": true,
    "path-intellisense.showHiddenFiles": true,
    "editor.inlineSuggest.fontFamily": "Monaspace Radon",
    "editor.quickSuggestions": {
        "strings": "on"
    },
    "editor.quickSuggestionsDelay": 5,
    "editor.suggest.matchOnWordStartOnly": false,
    "editor.suggest.showStatusBar": true,
    "files.autoGuessEncoding": true,
    "diffEditor.ignoreTrimWhitespace": true,
    "animations.Command-Palette": "Fade",
    "animations.Tabs": "Flip",
    "github.copilot.enable": {
        "*": true,
        "plaintext": false,
        "markdown": true,
        "scminput": false
    },
    "excalidraw.theme": "auto",
    "editor.unicodeHighlight.invisibleCharacters": false,
    "vue.server.hybridMode": true,
    "cmake.showOptionsMovedNotification": false,
    "latex-utilities.countWord.format": "${wordsBody} Words",
    "redhat.telemetry.enabled": true,
    "[latex]": {
        "editor.formatOnPaste": false,
        "editor.suggestSelection": "recentlyUsedByPrefix"
    },
    "latex-workshop.formatting.latex": "latexindent",
    "latex-workshop.bibtex-format.sort.enabled": true,
    "[html]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "markdown.extension.print.imgToBase64": true,
    "C_Cpp.intelliSenseEngine": "disabled",
    "C_Cpp.default.cppStandard": "c++14",
    "C_Cpp_Runner.cppStandard": "c++14",
    "C_Cpp_Runner.useAddressSanitizer": true,
    "C_Cpp_Runner.useLeakSanitizer": true,
    "[scss]": {
        "editor.defaultFormatter": "michelemelluso.code-beautifier"
    },
    "latex-workshop.latex.recipe.default": "lastUsed",
    "markdown-mermaid.lightModeTheme": "neutral",
    "markdown-mermaid.darkModeTheme": "default",
    "vscode-office.editorTheme": "Solarized",
    "gruvboxMaterial.diagnosticTextBackgroundOpacity": "25%",
    "workbench.preferredDarkColorTheme": "Gruvbox Material Dark",
    "workbench.colorTheme": "Gruvbox Material Dark",
    "gruvboxMaterial.lightWorkbench": "high-contrast",
    "gruvboxMaterial.darkWorkbench": "high-contrast",
    "workbench.iconTheme": "gruvbox-material-icon-theme",
    "errorLens.gutterIconsEnabled": true,
    "errorLens.statusBarIconsEnabled": true,
    "latex-workshop.view.pdf.invert": 0.8,
    "latex-workshop.view.pdf.invertMode.grayscale": 0.2,
    "errorLens.statusBarIconsUseBackground": false,
    "window.systemColorTheme": "auto",
    "latex-workshop.view.pdf.invertMode.enabled": "never",
    "latex-workshop.view.pdf.invertMode.brightness": 1.2,
    "editor.foldingImportsByDefault": true,
    "workbench.preferredLightColorTheme": "Gruvbox Material Light",
    "latex-workshop.view.pdf.color.dark.backgroundColor": "#f0f0f0",
    "gruvboxMaterial.darkCursor": "aqua",
    "gruvboxMaterial.darkSelection": "aqua",
    "gruvboxMaterial.lightSelection": "yellow",
    "gruvboxMaterial.lightCursor": "yellow",
    "C_Cpp.autoAddFileAssociations": false,
    "files.associations": {
        "*.bib": "bibtex",
        "*.zsh": "shellscript"
    },
    "latex-workshop.intellisense.citation.backend": "biblatex",
    "latex-workshop.intellisense.citation.label": "title",
    "[shellscript]": {
        "editor.defaultFormatter": "mads-hartmann.bash-ide-vscode"
    },
    "debug.console.wordWrap": false,
    "editor.inlayHints.fontFamily": "Monaspace Radon",
    "c-cpp-compile-run.c-flags": "-Wall -Wextra -g0 -o4",
    "c-cpp-compile-run.cpp-flags": "-Wall -Wextra -g0 -o4"
}
