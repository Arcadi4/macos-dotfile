{
    "[aslx]": {
        "editor.defaultFormatter": "DotJoshJohnson.xml"
    },
    "[css]": {
        "editor.defaultFormatter": "vscode.css-language-features"
    },
    "[latex]": {
        "editor.formatOnPaste": false,
        "editor.suggestSelection": "recentlyUsedByPrefix"
    },
    "[markdown]": {
        "editor.defaultFormatter": "yzhang.markdown-all-in-one",
        "editor.wordWrap": "bounded"
    },
    "[python]": {
        "diffEditor.ignoreTrimWhitespace": false,
        "editor.defaultColorDecorators": "never",
        "editor.defaultFormatter": "ms-python.autopep8",
        "editor.formatOnType": true,
        "editor.wordBasedSuggestions": "off",
        "gitlens.codeLens.symbolScopes": [
            "!Module"
        ]
    },
    "[r]": {
        "editor.wordSeparators": "`~!@#$%^&*()-=+[{]}\\|;:'\",<>/"
    },
    "[shellscript]": {
        "editor.defaultFormatter": "mads-hartmann.bash-ide-vscode"
    },
    "[toml]": {},
    "animations.Command-Palette": "Fade",
    "animations.Enabled": true,
    "animations.Install-Method": "Custom CSS and JS",
    "animations.Scrolling": "Fade",
    "animations.Tabs": "Flip",
    "azureResourceGroups.enableChatStandIn": false,
    "C_Cpp_Runner.cppStandard": "c++14",
    "C_Cpp_Runner.useAddressSanitizer": true,
    "C_Cpp_Runner.useLeakSanitizer": true,
    "C_Cpp.autoAddFileAssociations": false,
    "C_Cpp.default.cppStandard": "c++14",
    "C_Cpp.intelliSenseEngine": "disabled",
    "c-cpp-compile-run.c-flags": "-Wall -Wextra -g0 -o4",
    "c-cpp-compile-run.cpp-flags": "-Wall -Wextra -g0 -o4",
    "chat.agent.maxRequests": 50,
    "chat.editing.confirmEditRequestRemoval": false,
    "chat.editor.fontFamily": "Monaspace Neon",
    "chat.instructionsFilesLocations": {
        ".github/instructions": true
    },
    "chat.tools.urls.autoApprove": {},
